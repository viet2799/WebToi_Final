
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@using WebToi_final_01.Models;


<div class="content">

    <div class="module">
        <div class="module-head">
            <h3>Tables</h3>
        </div>
        <div class="module-body">
            <table class="table table-striped table-bordered table-condensed">
                <thead>
                    <tr>

                        <th>Mã danh mục</th>
                        <th>Tên danh mục</th>

                        <th colspan="3">Chức năng</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (DanhMuc it in Model)
                    {
                        <tr>

                            <td>@it.MaDM</td>
                            <td>@it.TenDM</td>

                            <td><a href="~/Admin/DanhMucAd/Edit/@it.MaDM">Sửa</a></td>
                            <td><a href="~/Admin/DanhMucAd/Delete/@it.MaDM">Xóa</a></td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>

        <div class="module-head">
            <h3></h3>
        </div>
    </div>



    <br />

</div>


